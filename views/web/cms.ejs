<script>
    $(function() {
        $(window).on('hashchange', function() {
            aimSection();
        });

        function aimSection() {
            var hash = location.hash;
            var aim = $(hash);
            var aimTop = aim.position().top - 70;
            document.body.scrollTop = document.body.scrollTop = aimTop;
        }
        window.onload = aimSection;
    });
</script>
